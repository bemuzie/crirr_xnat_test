<project>
  <extend>../plugin-parent/project.xml</extend>
  <pomVersion>3</pomVersion>
  <id>maven-jdeveloper-plugin</id>
  <name>Maven JDeveloper Plug-in</name>
  <currentVersion>1.4</currentVersion>
  <inceptionYear>2002</inceptionYear>
  <description>A plugin to generate various files for Oracle's JDeveloper IDE and ease the use of Maven within that environment</description>
  <shortDescription>JDeveloper Plugin for Maven</shortDescription>
  <url>http://maven.apache.org/reference/plugins/jdeveloper/</url>
  <issueTrackingUrl>http://jira.codehaus.org/BrowseProject.jspa?id=10376</issueTrackingUrl>
  <siteDirectory>/www/maven.apache.org/reference/plugins/jdeveloper/</siteDirectory>
  <repository>
    <connection>scm:cvs:pserver:anoncvs@cvs.apache.org:/home/cvspublic:maven-plugins/jdeveloper</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.apache.org:/home/cvs:maven-plugins/jdeveloper/</developerConnection>
    <url>http://cvs.apache.org/viewcvs/maven-plugins/jdeveloper/</url>
  </repository>
  <developers>
    <developer>
      <name>Tobias Rademacher</name>
      <id>tradem</id>
      <email>toby-wan-kenobi@web.de</email>
      <organization>GROB Werke</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
  </developers>
  <dependencies>
    <!-- Required for JavaPackageTool -->
    <!-- Don't upgrade to 2.5! -->

    <dependency>
      <groupId>jdepend</groupId>
      <artifactId>jdepend</artifactId>
      <version>2.3</version>
    </dependency>
  </dependencies>
  <versions>
    <version>
      <id>1.3</id>
      <name>1.3</name>
      <tag>MAVEN_JDEVELOPER_1_3</tag>
    </version>
    <version>
      <id>1.4</id>
      <name>1.4</name>
      <tag>MAVEN_JDEVELOPER_1_4</tag>
    </version>
  </versions>
</project>
