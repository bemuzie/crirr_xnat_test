#! /bin/sh# ----------------------------------------------------------------------------# Default JBoss start up shell script. It calls the default script found# in JBOSS_HOME/bin.# ----------------------------------------------------------------------------export JAVA_OPTS="-Djboss.server.home.dir=@jboss.server.home.dir@ -Djboss.server.home.url=@jboss.server.home.url@"@maven.jboss.home@/bin/run.sh -c @maven.jboss.conf.name@
