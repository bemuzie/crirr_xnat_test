  <developers>
    <developer>
      <name>Michal Maczka</name>
      <id>mmaczka</id>
      <email>michal.maczka@dimatics.com</email>
      <organization>Dimatics</organization>
      <roles>
        <role>Creator</role>
        <role>Release Manager</role>
        <role>Java Developer</role>
